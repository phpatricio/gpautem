<% content_for :content_header  do %>
  <section class="content-header">
    <h1>Listing <small>evaluacions</small></h1>

    <ol class="breadcrumb">
      <li><a href="/"><i class="fa fa-dashboard"></i> Home</a></li>
      <li class="active">evaluacions</li>
    </ol>    
  </section>
<% end %>

<section class="content">
  <div class="row">
    <div class="col-xs-12">
      <div class="box box-primary">
        <div class="box-header">
          <div class="box-tools pull-right">
            <%= link_to new_evaluacion_path(@evaluacion), :class => 'btn btn-success' do %>
              <span class="glyphicon glyphicon-plus"></span>
              New Evaluacion
            <% end %>          
          </div>
        </div>
        <div class="box-body table-responsive">
          <table class="table table-hover">
            <thead>
              <tr>
            
  <th>Pregunta 1</th>
            
                <th></th>
                <th></th>
                <th></th>
              </tr>
            </thead>
            <tbody>
            <% @evaluacions.each do |evaluacion| %>
            
  <td><%= evaluacion.pregunta_1 %></td>
            
                <td><%= link_to 'Show', evaluacion %></td>
                <td><%= link_to 'Edit', edit_evaluacion_path(evaluacion) %></td>
                <td><%= link_to 'Destroy', evaluacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              <% end %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</section>

